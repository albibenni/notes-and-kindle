  "scripts": {
    "note": "node --experimental-strip-types ./src/index.ts",
    "kindle": "node --experimental-strip-types ./src/prettify-kindle-notes.ts",
    "rm-dist": "rm -rf ./dist",
    "link-ts": "pnpm run rm-dist && tsc && pnpm link --global",
